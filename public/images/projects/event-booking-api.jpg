<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1A1F2B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2f3441;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Go API Structure -->
  <g transform="translate(100, 100)">
    <!-- API Endpoints -->
    <rect x="50" y="50" width="150" height="60" rx="8" fill="#00ADD8" opacity="0.8"/>
    <text x="125" y="85" text-anchor="middle" fill="white" font-family="Arial" font-size="12" font-weight="bold">Auth Endpoints</text>
    
    <rect x="250" y="50" width="150" height="60" rx="8" fill="#00ADD8" opacity="0.8"/>
    <text x="325" y="85" text-anchor="middle" fill="white" font-family="Arial" font-size="12" font-weight="bold">Event Endpoints</text>
    
    <rect x="450" y="50" width="150" height="60" rx="8" fill="#00ADD8" opacity="0.8"/>
    <text x="525" y="85" text-anchor="middle" fill="white" font-family="Arial" font-size="12" font-weight="bold">Booking Endpoints</text>
    
    <!-- Middleware Layer -->
    <rect x="50" y="150" width="550" height="40" rx="8" fill="#4E9AF1" opacity="0.8"/>
    <text x="325" y="175" text-anchor="middle" fill="white" font-family="Arial" font-size="14" font-weight="bold">JWT Authentication Middleware</text>
    
    <!-- Database -->
    <rect x="250" y="250" width="150" height="60" rx="8" fill="#E6B980" opacity="0.8"/>
    <text x="325" y="285" text-anchor="middle" fill="#1A1F2B" font-family="Arial" font-size="12" font-weight="bold">SQLite Database</text>
    
    <!-- Connection arrows -->
    <line x1="125" y1="110" x2="125" y2="150" stroke="#F1F1F1" stroke-width="2" marker-end="url(#arrowhead)"/>
    <line x1="325" y1="110" x2="325" y2="150" stroke="#F1F1F1" stroke-width="2" marker-end="url(#arrowhead)"/>
    <line x1="525" y1="110" x2="525" y2="150" stroke="#F1F1F1" stroke-width="2" marker-end="url(#arrowhead)"/>
    <line x1="325" y1="190" x2="325" y2="250" stroke="#F1F1F1" stroke-width="2" marker-end="url(#arrowhead)"/>
    
    <!-- Title -->
    <text x="325" y="30" text-anchor="middle" fill="#F1F1F1" font-family="Arial" font-size="20" font-weight="bold">Event Booking API</text>
    <text x="325" y="370" text-anchor="middle" fill="#F1F1F1" font-family="Arial" font-size="16">Golang • Gin Framework • JWT • SQLite</text>
  </g>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="0" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#F1F1F1" />
    </marker>
  </defs>
</svg>
